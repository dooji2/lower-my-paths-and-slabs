{
	"schemaVersion": 1,
	"id": "lmps",
	"version": "${version}",
	"name": "Lower My Paths & Slabs!",
	"description": "Lowers block models on top of dirt paths/slabs (toggleable)!",
	"authors": [
		"dooji"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/lmps",
		"sources": "https://github.com/dooji2/lower-my-paths-and-slabs"
	},
	"license": "GPL-v3",
	"icon": "assets/lmps/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.dooji.lmps.LMPS"
		],
		"client": [
			"com.dooji.lmps.LMPSClient"
		]
	},
	"mixins": [
		"lmps.mixins.json",
		{
			"config": "lmps.client.mixins.json",
			"environment": "client"
		},
		{
			"config": "lmps.sodium.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.3",
		"minecraft": "1.21.11",
		"java": ">=21",
		"fabric-api": ">=0.140.0+1.21.11"
	}
}
